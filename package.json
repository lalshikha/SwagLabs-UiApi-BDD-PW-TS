  "main": "index.js",
  "scripts": {
    "test": "cucumber-js",
    "test:smoke": "cucumber-js --require-module ts-node/register --config cucumber.conf.ts -t @smoke",
    "test:ui": "cucumber-js --require-module ts-node/register --config cucumber.conf.ts features/ui",
    "test:api": "cucumber-js --require-module ts-node/register --config cucumber.conf.ts features/functional -t @api",
    "test:parallel": "cucumber-js --require-module ts-node/register --config cucumber.conf.ts --parallel 3",
    "report": "allure generate reports/allure-results -o reports/allure-report --clean",
    "report:open": "powershell -Command Start-Process reports/allure-report",
    "report:html": "cucumber-html-reporter --input reports/cucumber-report.json --output reports/html/cucumber-report.html"
  },
  "keywords": [],
  "author": "",
